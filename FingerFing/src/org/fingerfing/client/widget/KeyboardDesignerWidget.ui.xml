<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:f="urn:import:org.fingerfing.client.widget">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:DecoratorPanel>
			<g:HorizontalPanel>
			<g:ListBox ui:field="fingerList" selectedIndex="0"></g:ListBox>
			<g:Button ui:field="nextFinger" text="Next Finger"/>
		</g:HorizontalPanel>
		</g:DecoratorPanel>
		<f:KeyboardWidget ui:field="keyboard"></f:KeyboardWidget>
	</g:HTMLPanel>
</ui:UiBinder> 